<h1 style="color: green; text-align: center">Student Registration</h1>
<form action="welcome.htm" method="post">
	<table bgcolor="pink" align="center">

		<tr>
			<td>Enter Name</td>
			<td><input type="text" name="sname"></td>
		</tr>
		<tr>
			<td>Enter Father Name</td>
			<td><input type="text" name="fname"></td>
		</tr>
		<tr>
			<td>Enter Mother Name</td>
			<td><input type="text" name="mname"></td>
		</tr>
		<tr>
			<td>Enter DOB</td>
			<td><input type="date" name="dob"></td>
		</tr>
		<tr>
			<td>Enter Address</td>
			<td><input type="text" name="addrs"></td>
		</tr>
		<tr>
			<td>Enter MaritalStatus</td>
			<td><input type="radio" name="ms" value="married">
		<tr>
			<td>Enter Pin</td>
			<td><input type="number" name="pin"></td>
		</tr>
		<tr>
			<td colspan="2">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input
				type="submit" value="submit"><input type="reset"
				value="cancel"></td>
		</tr>

	</table>


</form>